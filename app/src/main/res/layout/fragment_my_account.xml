<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FAF9F9"
    android:clickable="true"
    android:id="@+id/Rela_More">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">



    <androidx.appcompat.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="?actionBarSize"
        android:id="@+id/toolbar"
        android:background="@color/colorPrimary">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <ImageView
                android:id="@+id/textView"
                android:layout_width="@dimen/_60sdp"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:src="@drawable/logo_gzr"
                android:textColor="#fff"
                android:textSize="@dimen/_16sdp"
                android:textStyle="bold"
                />
        </RelativeLayout>
    </androidx.appcompat.widget.Toolbar>


    <androidx.cardview.widget.CardView
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_marginTop="@dimen/_25sdp"
        app:cardElevation="1dp"
        android:id="@+id/card_view"
        android:layout_below="@+id/toolbar"
        android:layout_marginBottom="@dimen/_5sdp"
        android:layout_height="@dimen/_90sdp">

      <LinearLayout
          android:layout_width="match_parent"
          android:weightSum="4"
          android:layout_marginTop="@dimen/_15sdp"
          android:layout_height="@dimen/_40sdp"
          >
          <ImageView
              android:id="@+id/Img_profile"
              android:layout_width="0dp"
              android:layout_height="@dimen/_40sdp"
              android:layout_weight="1"
              android:src="@drawable/ic_myprofile" />

          <ImageView
              android:id="@+id/Img_Promo"
              android:layout_width="0dp"
              android:layout_height="@dimen/_40sdp"
              android:layout_weight="1"
              android:src="@drawable/ic_promo" />

          <ImageView
              android:id="@+id/Img_WishLst"
              android:layout_width="0dp"
              android:layout_height="@dimen/_40sdp"
              android:layout_weight="1"
              android:src="@drawable/ic_wishlist" />
          <ImageView
              android:id="@+id/Img_Orders"
              android:layout_width="0dp"
              android:layout_height="@dimen/_40sdp"
              android:layout_weight="1"
              android:src="@drawable/ic_orders" />


      </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:weightSum="4"
            android:layout_marginTop="@dimen/_65sdp"
            android:layout_height="wrap_content"
            >

            <TextView
                android:id="@+id/T_MyProfile"
                android:layout_width="0dp"
                android:gravity="center"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/myprofile"
                android:textColor="#000"
                android:textSize="@dimen/_12sdp"
                />


            <TextView
                android:id="@+id/T_Promo"
                android:layout_width="0dp"
                android:gravity="center"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/promo"
                android:textColor="#000"
                android:textSize="@dimen/_12sdp"
                />

            <TextView
                android:id="@+id/T_Wishlist"
                android:layout_width="0dp"
                android:gravity="center"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/wishlist"
                android:textColor="#000"
                android:textSize="@dimen/_12sdp"
                />

            <TextView
                android:id="@+id/T_Orders"
                android:layout_width="0dp"
                android:gravity="center"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/recentorders"
                android:textColor="#000"
                android:textSize="@dimen/_12sdp"
                />

        </LinearLayout>


    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_marginTop="@dimen/_25sdp"
        app:cardElevation="1dp"
        android:id="@+id/card_Login"
        android:visibility="invisible"
        android:layout_below="@+id/toolbar"
        android:layout_marginBottom="@dimen/_5sdp"
        android:layout_height="@dimen/_90sdp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/Img_Login"
                android:layout_width="@dimen/_45sdp"
                android:layout_height="@dimen/_40sdp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/_15sdp"
                android:src="@drawable/ic_myprofile" />

            <TextView
                android:id="@+id/T_Login"
                android:layout_width="wrap_content"
                android:gravity="center"
                android:layout_below="@+id/Img_Login"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:text="@string/login"
                android:textColor="#000"
                android:textSize="@dimen/_12sdp"
                />

        </RelativeLayout>



    </androidx.cardview.widget.CardView>

